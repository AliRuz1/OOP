public class Text {
    private String text;

    Text(String text) {
        this.text = text;
    }

    public String getText() {
        return text;
    }

    public void newText(String text) {
        StringBuilder strb = new StringBuilder(this.text); //this
        strb.append(' ');
        this.text = strb.append(text).toString();

    }
}

